@using Entities
@model IEnumerable<Game>
<div id="searchresults">
    @if (Model.Count() > 0)
    {
        <h3>Все книги автора : @Model.First().Name</h3>
        <ul>
            @foreach (var item in Model)
            {
                <li>@item.Name</li>
            }
        </ul>
    }
</div>
